{
  "kind": "collectionType",
  "collectionName": "usinas",
  "info": {
    "singularName": "usina",
    "pluralName": "usinas",
    "displayName": "Usina",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "titulo": {
      "type": "string",
      "required": true
    },
    "media": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images", "files", "videos"]
    },
    "usinaID": {
      "type": "uid"
    },
    "aprobado": {
      "type": "enumeration",
      "enum": ["aprobada", "pendiente", "rechazada"],
      "default": "pendiente",
      "required": true
    },
    "creador": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "usinas_creadas"
    },
    "notificaciones": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notificacion.notificacion",
      "mappedBy": "usinaAfectada"
    }
  }
}
